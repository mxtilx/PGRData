    {
        "Id": 62,
        "ActionId": 62,
        "NextActionId": null,
        "Type": 301,
        "IsBlock": null,
        "IsEnd": null,
        "BeginAnim": null,
        "EndAnim": null,
        "BeginDelay": null,
        "EndDelay": null,
        "Params": [
            "Flora",
            "My friends and I, as well as our parents, would gather and watch the opera on a big screen, guided by Pieter's narration.",
