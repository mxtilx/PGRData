    },
    {
        "Id": 27,
        "ActionId": 27,
        "NextActionId": null,
        "Type": 301,
        "IsBlock": null,
        "IsEnd": null,
        "BeginAnim": null,
        "EndAnim": null,
        "BeginDelay": null,
        "EndDelay": null,
        "Params": [
