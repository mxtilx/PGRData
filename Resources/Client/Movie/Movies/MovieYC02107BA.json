        "IsEnd": null,
        "BeginAnim": null,
        "EndAnim": null,
        "BeginDelay": null,
        "EndDelay": null,
        "Params": [
            "Medical Director",
            "Ma'am, I am sorry. But I have to say this.",
            1.0
        ]
    },
    {
