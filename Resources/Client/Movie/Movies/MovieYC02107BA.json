        "IsBlock": null,
        "IsEnd": null,
        "BeginAnim": null,
        "EndAnim": null,
        "BeginDelay": null,
        "EndDelay": null,
        "Params": [
            "Lustrous",
            "I'm going back to work. Is there anything else you want to say?",
            2.0
        ]
    },
    {
        "Id": 81,
        "ActionId": 81,
        "NextActionId": null,
        "Type": 301,
