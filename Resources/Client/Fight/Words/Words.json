    {
        "Group":15041001,
        "WordsIndex":1,
        "Text":"I am the Alpha and the Omega. I am the arbiter. I am everything.",
        "Duration":11.0,
        "CueId":100000106.0
    },
    {
        "Group":15041002,
        "WordsIndex":1,
        "Text":"I am the cunning fox. I am the dreadful lion.",
